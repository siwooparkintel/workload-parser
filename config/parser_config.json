{
  "parsers": {
    "power": {
      "name": "power",
      "enabled": true,
      "file_patterns": [
        "*power*.csv",
        "*Power*.csv",
        "*pwr*.csv",
        "*energy*.csv"
      ],
      "options": {
        "delimiter": ",",
        "skip_rows": 0,
        "encoding": "utf-8"
      }
    },
    "etl": {
      "name": "etl",
      "enabled": true,
      "file_patterns": [
        "*etl*.csv",
        "*ETL*.csv",
        "*extract*.csv",
        "*transform*.csv",
        "*load*.csv"
      ],
      "options": {
        "delimiter": ",",
        "skip_rows": 0,
        "encoding": "utf-8"
      }
    },
    "socwatch": {
      "name": "socwatch",
      "enabled": true,
      "file_patterns": [
        "*socwatch*.csv",
        "*SocWatch*.csv",
        "*soc*watch*.csv",
        "*perf*monitor*.csv"
      ],
      "options": {
        "delimiter": ",",
        "skip_rows": 0,
        "encoding": "utf-8"
      }
    }
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "console": true,
    "file_path": "logs/workload_parser.log"
  },
  "output": {
    "format": "json",
    "include_raw_data": false,
    "output_directory": "output"
  }
}